<!DOCTYPE html>
<html>
<head>
  <title>Leituras</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |type, msg| %>
    <div class= "alert-info">
      <%= msg %>
    </div>
  <% end %>
<div class= 'container'>
  <%= yield %>
</div>
</body>
</html>
